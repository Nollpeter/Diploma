<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
        <LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
        <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
        <WasmEmitSymbolMap>true</WasmEmitSymbolMap>
    </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Blazored.LocalStorage" Version="4.4.0" />
		<PackageReference Include="bunit" Version="1.23.9" />
		<PackageReference Include="Dropbox.Api" Version="6.37.0" />
		<PackageReference Include="FluentAssertions" Version="6.12.0" />
		<PackageReference Include="FluentAssertions.Extensions" Version="1.0.18" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.11" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="7.0.11" PrivateAssets="all" />
		<PackageReference Include="MudBlazor" Version="6.11.0" />
		<PackageReference Include="NSubstituteNet6" Version="4.3.0" />
		<PackageReference Include="NUnit" Version="4.0.0-beta.1" />
	</ItemGroup>

	<ItemGroup>
	  <!--<Folder Include="Shared\Examples\7_DependencyInjection\ExampleUsages\" />-->
	  <Folder Include="Shared\_Exercises\1_Components\4_LifeCycle\" />
	  <Folder Include="Tests\Helpers\" />
	  <Folder Include="wwwroot\img\" />
	</ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="**\*.cs" />
        <EmbeddedResource Include="**\*.js" />
        <EmbeddedResource Include="**\*.razor" />
    </ItemGroup>

