@Title
@foreach (var item in ListItems)
{
    @ListItemTemplate(item) //Notice the new rendering logic here
}
@code{

    [Parameter, EditorRequired] public RenderFragment Title { get; set; }
    [Parameter, EditorRequired] public RenderFragment<TestClass> ListItemTemplate { get; set; } // Notice the new property here
    [Parameter, EditorRequired] public List<TestClass> ListItems { get; set; }

}